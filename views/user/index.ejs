        <script>
             function addToCart(proId) {
            // console.log("api vall");
            console.log(proId);
            
            $.ajax ({
                url: `/add-to-cart/${proId}`,
                method: 'get',
                success: (response) => {
                    // console.log(response);
                    if(response.status) {
                        // alert("wowww")
                        // location.reload();
                        Toastify({
                            text: "ADDED TO YOUR CART",
                            duration: 3000,
                            gravity: "bottom",
                            position: "center",
                            style: {
                                background: "linear-gradient(to right, #00b09b, #96c93d)",
                            }
                            }).showToast();
                           
                    }
                }
            })
        }
        </script>